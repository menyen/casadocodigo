<html>

<head></head>

<body>
    <h1>Lista de produtos</h1>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>Título</th>
                <th>Descrição</th>
                <th>Preço</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i = 0; i < produtos.length; i++) { %>
                <tr>
                    <td><%= produtos[i].id %></td>
                    <td><%= produtos[i].titulo %></td>
                    <td><%= produtos[i].descricao %></td>
                    <td><%= produtos[i].preco %></td>
                </tr>

                <% } %>
        </tbody>
    </table>
</body>

</html>